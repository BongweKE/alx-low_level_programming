#include <stdlib.h>
#include <time.h>

/**
 * main - entry point
 * Description: Test if a random integer is ><= 0
 *
 */
int main(void)
{
	int n;

	srand(time(0));
	n = rand() - RAND_MAX / 2;
	if (n > 0)
	{
		print("%d is positive\n", n);
	}
	else if (n < 0)
	{
		print("%d is negative\n",);
	}
	else
	{
		print("%d id zero\n",);
	}
}
